      <li>NamerMinotaurUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=12)->[MinoFirstMale]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerMinotaurFemale</defName>
    <include>
      <li>NamerMinotaurUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=12)->[MinoFirstFemale]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

</Defs>