    <defName>NamerMinotaurUtility</defName>
    <rulePack>
      <rulesFiles>
        <li>MinoFirstMale->NameBanks/Names_First_Minotaur_Male</li>
        <li>MinoFirstFemale->NameBanks/Names_First_Minotaur_Female</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>NamerMinotaurMale</defName>
